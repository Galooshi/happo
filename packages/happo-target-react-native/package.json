  "devDependencies": {
    "react-native": "^0.40.0",
    "react-native-view-shot": "^1.5.1",
    "react-native-windows": "^0.43.0-rc.0",
    "rnpm-plugin-windows": "^0.2.4"
  },
  "peerDependencies": {
    "react-native": "*",
    "react-native-view-shot": "*"
  },
  "dependencies": {
    "appium": "^1.6.3",
    "express": "^4.14.0",
    "happo-core": "^5.0.0-rc.1",
    "mkdirp": "^0.5.1",
    "react": "15.4.1",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "wd": "^1.1.1"
  }
}
