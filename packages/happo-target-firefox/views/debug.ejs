<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Happo Debug Tool</title>

    <link rel="shortcut icon" href="<%= favicon %>" />

    <style>
      <%- css %>
    </style>

    <script src="https://unpkg.com/react@15.4.1/dist/react.min.js"></script>
    <script src="https://unpkg.com/react-dom@15.4.1/dist/react-dom.min.js"></script>

    <script>
      <%- jsCode %>
    </script>
    <script src="/HappoRunner.bundle.js"></script>

    <% sourceFiles.forEach(function(sourceFile) { %>
      <script src="/resource?file=<%= encodeURIComponent(sourceFile) %>"></script>
    <% }); %>
  </head>

  <body>
    <div id="react-root"></div>
  </body>
</html>
